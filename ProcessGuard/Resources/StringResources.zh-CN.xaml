<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="ServiceConfig">进程守护服务配置</system:String>
    <system:String x:Key="GlobalConfig">全局配置</system:String>
    <system:String x:Key="FullPathOfExecutable">可执行文件完整路径</system:String>
    <system:String x:Key="InputOrSelectTheFullPath">填写或者选取完整路径</system:String>
    <system:String x:Key="SelectUsingFileExplorer">使用文件资源管理器进行选取</system:String>
    <system:String x:Key="ProcessName">进程名称</system:String>
    <system:String x:Key="StartOnlyOnce">仅启动一次</system:String>
    <system:String x:Key="MinimizeStartup">启动时最小化</system:String>
    <system:String x:Key="NoWindow">无窗应用</system:String>
    <system:String x:Key="StartupParameters">启动参数</system:String>
    <system:String x:Key="CanBeLeftBlank">可为空</system:String>
    <system:String x:Key="ConfirmAdd">确认添加</system:String>
    <system:String x:Key="ConfirmChanges">确认修改</system:String>
    <system:String x:Key="Cancel">取消</system:String>
